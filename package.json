{
    "name": "api-next-typescript",
    "version": "1.0.6",
    "private": true,
    "description": "Armado de api utilizando TS y NextJS y React",
    "scripts": {
        "dev": "NODE_OPTIONS='--inspect'; next dev --port ${PORT-3000}",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "release:beta": "npx standar-version --prerelease beta",
        "release": "standard-version",
        "frendly": "commitizen init cz-conventional-changelog --save-dev --save-exact",
        "alias": "git config â€”global alias.cz git-cz",
        "postInstall": "npm -g commitizen; npm run frendly; npm run alias",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git@GitHub:jmz-student/api-next-typescript.git"
    },
    "author": "Jorge Mendez Ortega <jorge.mendez.ortega@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "mongoose": "6.2.11",
        "next": "12.1.5",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "react-is": "18.0.0",
        "styled-components": "5.3.5"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.4",
        "@commitlint/config-conventional": "16.2.4",
        "@types/cli-color": "2.0.2",
        "@types/node": "17.0.23",
        "@types/react": "18.0.4",
        "@types/styled-components": "5.1.25",
        "@typescript-eslint/eslint-plugin": "5.19.0",
        "@typescript-eslint/parser": "5.19.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "8.13.0",
        "eslint-config-next": "12.1.5",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.29.4",
        "husky": "7.0.4",
        "lint-staged": "12.4.1",
        "prettier": "2.6.2",
        "standard-version": "9.3.2",
        "typescript": "4.6.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
